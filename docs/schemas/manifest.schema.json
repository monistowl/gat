{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "ChunkState": {
      "properties": {
        "completed_at": {
          "type": [
            "string",
            "null"
          ]
        },
        "id": {
          "type": "string"
        },
        "status": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "status"
      ],
      "type": "object"
    },
    "InputArtifact": {
      "properties": {
        "hash": {
          "type": [
            "string",
            "null"
          ]
        },
        "path": {
          "type": "string"
        }
      },
      "required": [
        "path"
      ],
      "type": "object"
    },
    "ManifestTelemetry": {
      "properties": {
        "correlation_id": {
          "type": [
            "string",
            "null"
          ]
        },
        "duration_ms": {
          "format": "uint128",
          "minimum": 0.0,
          "type": [
            "integer",
            "null"
          ]
        },
        "env": {
          "items": {
            "$ref": "#/definitions/Param"
          },
          "type": "array"
        },
        "status": {
          "type": "string"
        }
      },
      "required": [
        "status"
      ],
      "type": "object"
    },
    "Param": {
      "properties": {
        "name": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "value"
      ],
      "type": "object"
    }
  },
  "properties": {
    "chunk_map": {
      "default": [],
      "items": {
        "$ref": "#/definitions/ChunkState"
      },
      "type": "array"
    },
    "command": {
      "type": "string"
    },
    "inputs": {
      "default": [],
      "items": {
        "$ref": "#/definitions/InputArtifact"
      },
      "type": "array"
    },
    "outputs": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "params": {
      "items": {
        "$ref": "#/definitions/Param"
      },
      "type": "array"
    },
    "run_id": {
      "type": "string"
    },
    "seed": {
      "type": [
        "string",
        "null"
      ]
    },
    "telemetry": {
      "allOf": [
        {
          "$ref": "#/definitions/ManifestTelemetry"
        }
      ],
      "default": {
        "status": ""
      }
    },
    "timestamp": {
      "type": "string"
    },
    "version": {
      "type": "string"
    }
  },
  "required": [
    "command",
    "outputs",
    "params",
    "run_id",
    "timestamp",
    "version"
  ],
  "title": "ManifestEntry",
  "type": "object"
}